<%= form_for(@workout_log) do |f| %>
  <% if @workout_log.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@workout_log.errors.count, "error") %> prohibited this workout_log from being saved:</h2>

      <ul>
      <% @workout_log.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :workout_id %>
    <div class="form-select">
      <select name="workout_log[workout_id]" id="workout_log_workout_id">
      <% @workouts.each do |workout| %>
        <option value="<%= workout.id %>"><%= workout.workout_name %></option>
      <% end %>
      </select>
    </div>
  </div>
  


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
